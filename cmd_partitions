/* CRIANDO PARTIRTION */
ALTER TABLE <TABELA> ADD PARTITION <NOME> VALUES <VALORCAMPO>;

/* CRIANDO SUBPARTIRTION */ 
ALTER TABLE <TABELA>
   MODIFY PARTITION <NOME> ADD SUBPARTITION <NOME_SUB>  VALUES (TO_DATE(' 2019-07-04 00:00:00', 'SYYYY-MM-DD HH24:MI:SS', 'NLS_CALENDAR=GREGORIAN'));
  
/* TRUNCANDO PARTIRTION */
ALTER TABLE <TABLE> TRUNCATE PARTITION <NOME_PARTITION>;
